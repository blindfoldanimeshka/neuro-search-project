{
  "projectName": "nalog-obnal",
  "description": "Система поиска и анализа товаров с AI-помощником",
  "stages": [
    {
      "name": "setup",
      "description": "Настройка проекта и зависимостей",
      "tasks": [
        "npm install",
        "npm run dev"
      ]
    },
    {
      "name": "development",
      "description": "Разработка функциональности",
      "tasks": [
        "npm run dev",
        "npm run build"
      ]
    },
    {
      "name": "testing",
      "description": "Тестирование приложения",
      "tasks": [
        "npm test",
        "npm run test:watch"
      ]
    },
    {
      "name": "deployment",
      "description": "Развертывание приложения",
      "tasks": [
        "npm run build",
        "npm run start"
      ]
    }
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "next": "^14.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tailwindcss": "^3.0.0",
    "lucide-react": "^0.300.0",
    "react-range": "^1.8.0",
    "xlsx": "^0.18.0",
    "file-saver": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "typescript": "^5.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.0.0"
  }
} 